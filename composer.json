{
    "name": "mysteryreloaded/laraone-theme",
    "description": "Support files for laraone theme development",
    "license": "MIT",
    "authors": [
        {
            "name": "mysteryreloaded",
            "email": "ntrifunovicsrb@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "mysteryreloaded\\laraonetheme\\ThemeServiceProvider"
            ],
            "aliases": {
                "ThemeFiles": "mysteryreloaded\\laraonetheme\\Facades\\ThemeFiles"
            }
        }
    },
    "require-dev": {
		"phpunit/phpunit": "6.*",
